<rdfq:rdfquery>
  <rdfq:From eachResource="Files">
    <rdfq:Select>
      <rdfq:Condition>
        <rdfq:and>	

          <rdfq:greaterThan>
            <rdfq:Property name="Audio.ReleaseDate" />
            <rdf:Integer>1979</rdf:Integer> 
          </rdfq:greaterThan>

          <rdfq:lessThan>
            <rdfq:Property name="Audio.ReleaseDate" />
            <rdf:Integer>1990</rdf:Integer> 
          </rdfq:lessThan>

    
       </rdfq:and>
     </rdfq:Condition>
   </rdfq:Select>
  </rdfq:From>
</rdfq:rdfquery>
